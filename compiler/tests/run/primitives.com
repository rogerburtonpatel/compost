;; Test each primitive operator, except for 
;; the I/O ones 

(: main (-> () unit))
(define main ()
    (begin 
        ;; Equality 
        (print-bool (=i 2 2))
        (print-newline)
        (print-bool (=i 2 3))
        (print-newline)
        (print-bool (=s 'Hello' 'Hello'))
        (print-newline)
        (print-bool (=s 'Hello' 'Hello1'))
        (print-newline)
        (print-bool (=b true true))
        (print-newline)
        (print-bool (=b true false))
        (print-newline)
        (print-bool (=u unit unit))
        (print-newline)
        (print-bool (=b false true))
        (print-newline)
        ;; Arithmetic
        (print-int (+ 2 3))
        (print-newline)
        (print-int (+ -2 -3))
        (print-newline)
        (print-int (- 2 3))
        (print-newline)
        (print-int (- -2 -3))
        (print-newline)
        (print-int (* 2 3))
        (print-newline)
        (print-int (* 2 -3))
        (print-newline)
        (print-int (/ 5 2))
        (print-newline)
        (print-int (/ -2 -1))
        (print-newline)
        (print-int (% 5 2))
        (print-newline)
        (print-int (% -2 -1))
        (print-newline)
        (print-int (udiv 5 2))
        (print-newline)
        (print-int (udiv -2 -1))
        (print-newline)
        (print-int (umod 5 2))
        (print-newline)
        (print-int (umod -2 -1))
        (print-newline)
        (print-int (neg 2))
        (print-newline)
        (print-int (neg -2))
        (print-newline)
        ;; Comparison 
        (print-bool (> -5 -2))
        (print-newline)
        (print-bool (> -2 -5))
        (print-newline)
        (print-bool (< -5 -2))
        (print-newline)
        (print-bool (< -2 -5))
        (print-newline)
        (print-bool (>= -5 -2))
        (print-newline)
        (print-bool (>= -2 -5))
        (print-newline)
        (print-bool (<= -5 -2))
        (print-newline)
        (print-bool (<= -2 -5))
        (print-newline)
        (print-bool (>= 0 0))
        (print-newline)
        (print-bool (> 0 0))
        (print-newline)
        (print-bool (<= 0 0))
        (print-newline)
        (print-bool (< 0 0))
        (print-newline)
        ;; Boolean
        (print-bool (not false))
        (print-newline)
        (print-bool (not true))
        (print-newline)
        (print-bool (and true true))
        (print-newline)
        (print-bool (and true false))
        (print-newline)
        (print-bool (and false false))
        (print-newline)
        (print-bool (or true true))
        (print-newline)
        (print-bool (or false true))
        (print-newline)
        (print-bool (or false false))
        (print-newline)
        (print-bool (xor true true))
        (print-newline)
        (print-bool (xor true false))
        (print-newline)
        (print-bool (xor false true))
        (print-newline)
        (print-bool (xor false false))
        (print-newline)
        ;; Bitwise 
        (print-int (& 5 3))
        (print-newline)
        (print-int (| 5 3))
        (print-newline)
        (print-int (^ 5 3))
        (print-newline)
        (print-int (<< 1 2))
        (print-newline)
        (print-int (>> 4 2))
        (print-newline)
        (print-int (<< -1 2))
        (print-newline)
        ;(print-int (>> -4 2)) ;; appears to be machine dependent
        ;(print-newline)
        (print-int (~ 15))
        (print-newline)
        (print-int (~ -16))
        (print-newline)))
